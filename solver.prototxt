train_net: "mini_train_test.prototxt"
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 5000
# Display every 20 iterations
display: 20
# The maximum number of iterations
max_iter: 20000
# snapshot intermediate results
snapshot: 5000
snapshot_prefix: "c3d_ucf101_finetune_whole"
# solver mode: CPU or GPU
solver_mode: CPU
